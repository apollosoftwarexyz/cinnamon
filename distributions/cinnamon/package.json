{
  "name": "@apollosoftwarexyz/cinnamon",
  "version": "0.0.5",
  "author": "Apollo Software Limited",
  "description": "Apollo Software's intuitive backend API framework.",
  "license": "MIT",
  "main": "dist/index.cjs",
  "types": "types/index.cjs.d.ts",
  "exports": {
    ".": "./dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollosoftwarexyz/cinnamon.git",
    "directory": "distributions/cinnamon/"
  },
  "scripts": {
    "clean": "rm -rf types/; rm -rf dist/;",
    "build": "tsc --build",
    "prerelease": "ts-node --project tsconfig.utils.json utils/prerelease.ts",
    "prepublishOnly": "cp ../../README.md ./README.md; yarn prerelease",
    "doPublish": "npm publish --access public"
  },
  "peerDependencies": {
    "@mikro-orm/mongodb": "~4.5.9",
    "@mikro-orm/postgresql": "~4.5.9",
    "@mirko-orm/mariadb": "~4.5.9"
  }
}
