{
  "name": "@apollosoftwarexyz/cinnamon",
  "includeVersion": true,
  "tsconfig": "tsconfig.build.json",
  "entryPointStrategy": "Packages",
  "out": "docs/static/api",
  "mergeModulesMergeMode": "project",
  "sort": [
    "enum-value-ascending",
    "visibility",
    "source-order",
    "static-first",
    "instance-first",
    "required-first",
    "alphabetical"
  ],
  "excludePrivate": true,
  "excludeInternal": true
}
